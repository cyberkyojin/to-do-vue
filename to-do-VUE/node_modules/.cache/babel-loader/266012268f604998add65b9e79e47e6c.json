,EAAxC;AAFC,OAAD,CAAL;AAID,KA5BM;AA6BP,IAAA,QA7BO,oBA6BE,EA7BF,EA6BM;AACX,MAAA,KAAK,CAAC;AACJ,QAAA,MAAM,EAAE,OADJ;AAEJ,QAAA,GAAG,yCAAkC,EAAlC,CAFC;AAGJ,QAAA,IAAI,EAAE;AACJ,UAAA,IAAI,EAAE,KAAK;AADP;AAHF,OAAD,CAAL;AAOD,KArCM;AAsCP,IAAA,KAtCO,iBAsCD,IAtCC,EAsCK;AACV,WAAK,IAAL,GAAY,CAAC,KAAK,IAAlB;AACA,WAAK,KAAL,GAAa,IAAb;AACD;AAzCM,GARI;AAmDb,EAAA,OAnDa,qBAmDH;AAAA;;AACR,IAAA,KAAK,CAAC,GAAN,CAAU,wBAAV,EAAoC,IAApC,CAAyC,UAAC,CAAD,EAAO;AAC9C,MAAA,CAAC,CAAC,IAAF,CAAO,KAAP,CAAa,OAAb,CAAqB,UAAA,CAAA;AAAA,eAAK,KAAI,CAAC,KAAL,CAAW,IAAX,CAAgB,CAAhB,CAAL;AAAA,OAArB;AACD,KAFD;AAGD,GAvDY;AAyDb,EAAA,KAAK,EAAE;AACL,IAAA,KAAK,EAAE;AACL,MAAA,OADK,mBACG,GADH,EACQ,MADR,EACgB;AACnB,QAAA,OAAO,CAAC,GAAR,CAAY,mBAAZ;AACD,OAHI;AAIL,MAAA,IAAI,EAAE;AAJD;AADF;AAzDM,CAAf","sourcesContent":["<template>\n  <form v-if=\"edit\" @submit=\"editTask\">\n    <input type=\"text\" placeholder=\"Edit task\" v-model=\"taskE\" required />\n    <button type=\"submit\"><i class=\"bi bi-plus-circle-dotted\"></i></button>\n  </form>\n\n  <form v-else @submit=\"addTask\">\n    <input type=\"text\" placeholder=\"New task\" v-model=\"task\" required />\n    <button type=\"submit\"><i class=\"bi bi-plus-circle-dotted\"></i></button>\n  </form>\n\n  <h2>Tasks</h2>\n  <div v-for=\"(task, i) in taskL\" :key=\"i\" id=\"task\">\n    {{ task.task }}\n\n    <!-- <span\n      :style=\"task.completed == true ? 'text-decoration: line-through' : ''\"\n      v-on:click=\"completed(task._id, task.completed)\"\n    >\n    </span> -->\n    <i class=\"bi bi-x-lg\" v-on:click=\"rmTask(task._id)\"></i>\n    <i class=\"bi bi-pencil-square\" v-on:click=\"editI(task.task)\"></i>\n  </div>\n</template>\n\n<script>\n// import { v4 as uuidv4 } from \"uuid\";\n// import api from \"@/services/api.js\";\nimport axios from \"axios\";\nimport { watch } from '@vue/runtime-core';\n\nexport default {\n  name: \"ToDo\",\n  data() {\n    return {\n      taskL: [],\n      edit: false,\n    };\n  },\n  methods: {\n    addTask(e) {\n      e.preventDefault();\n\n      this.taskL.push({task: this.task})\n      console.log(this.taskL)\n      axios({\n        method: \"post\",\n        url: \"http://localhost:8069/create\",\n        data: {\n          task: this.task,\n        },\n      });\n\n      this.task = \"\";\n    },\n    rmTask(id) {\n      axios({\n        method: \"delete\",\n        url: `http://localhost:8069/delete/${id}`,\n      });\n    },\n    completed(id, trueOrFalse) {\n      const tf = !trueOrFalse;\n      axios({\n        method: \"patch\",\n        url: `http://localhost:8069/update/${id}/${tf}`,\n      });\n    },\n    editTask(id) {\n      axios({\n        method: \"patch\",\n        url: `http://localhost:8069/update/${id}`,\n        data: {\n          task: this.taskE,\n        },\n      });\n    },\n    editI(task) {\n      this.edit = !this.edit;\n      this.taskE = task;\n    },\n  },\n  created() {\n    axios.get(\"http://localhost:8069/\").then((r) => {\n      r.data.tasks.forEach(t => this.taskL.push(t))\n    })\n  },\n  \n  watch: {\n    taskL: {\n      handler(val, oldVal) {\n        console.log('book list changed')\n      },\n      deep: true\n    },\n  }\n  \n};\n</script>\n\n<style scoped>\nh2 {\n  border-bottom: 1px solid #e858cb;\n  color: #e858cb;\n}\n\ni {\n  cursor: pointer;\n  margin-right: 3px;\n}\n\n#task {\n  background-color: white;\n  font-size: 20px;\n  margin-bottom: 3px;\n}\n\nspan {\n  cursor: pointer;\n  margin-right: 60px;\n}\n</style>"],"sourceRoot":""}]}