\n\n      this.task = \"\";\n    },\n    rmTask(id) {\n      axios({\n        method: \"delete\",\n        url: `http://localhost:8069/delete/${id}`,\n      });\n    },\n    completed(id, trueOrFalse) {\n      const tf = !trueOrFalse;\n      axios({\n        method: \"patch\",\n        url: `http://localhost:8069/update/${id}/${tf}`,\n      });\n    },\n    editTask(id) {\n      axios({\n        method: \"patch\",\n        url: `http://localhost:8069/update/${id}`,\n        data: {\n          task: this.taskE,\n        },\n      });\n    },\n    editI(task) {\n      this.edit = !this.edit;\n      this.taskE = task;\n    },\n  },\n  mounted() {\n    axios.get(\"http://localhost:8069/\").then((r) => {\n      this.taskL = r.data.tasks;\n    });\n  },\n};\n</script>\n\n<style scoped>\nh2 {\n  border-bottom: 1px solid #e858cb;\n  color: #e858cb;\n}\n\ni {\n  cursor: pointer;\n  margin-right: 3px;\n}\n\n#task {\n  background-color: white;\n  font-size: 20px;\n  margin-bottom: 3px;\n}\n\nspan {\n  cursor: pointer;\n  margin-right: 60px;\n}\n</style>"]}]}